## 128bit
t 2
n 01110101001001000111001011000100011101010010010001110010110001001010101001110100101001010101101011100101001010011101010111101010

# word swap
r 0 128 64
e 10101010011101001010010101011010111001010010100111010101111010100111010100100100011100101100010001110101001001000111001011000100

# swap back
r 0 128 -64
e 01110101001001000111001011000100011101010010010001110010110001001010101001110100101001010101101011100101001010011101010111101010

# 32bit rot on 64-bit substring
r 0 64 32
e 01110101001001000111001011000100011101010010010001110010110001001010101001110100101001010101101011100101001010011101010111101010

# 32bit rot on 64-bit substring unaligned
r 10 64 -32
e 01110101001001000111001011000100101010100110010001110010110001000111010100110100101001010101101011100101001010011101010111101010

# 31bit rot on 32-bit aligned bitstring
r 31 64 31
e 01110101001001000111001011000100111010100110100101001010101101010101010011001000111001011000100011100101001010011101010111101010


