# cross bytes, short rotation
t 0

# 1001[|01101]011
n 100101101011
r 4 5 3
e 100110101011

# 1001[01101]011
n 100101101011
r 4 5 13
e 100110101011

# 1001[01101]011
n 100101101011
r 4 5 -12
e 100110101011

# cross bytes, full bytes
t 1

# [10010110 | 10110101]
n 1001011010110101 
r 0 16 8
e 1011010110010110

t 3

# 10010110 [1011|0101 10000101] 01011010
n 10010110101101011000010101011010
r 8 16 -4
# 10010110 [01011000 0101|1011] 01011010
e 10010110010110000101101101011010

# with surplus, head > tail
t 4

# 1001[0110 10|110101 10000101 010]11010
n 10010110101101011000010101011010
r 4 23 -6
# 1001[1101 01100001 01010|011 010]11010
e 10011101011000010101001101011010

# with surplus, tail >= head
t 5

# 100101[10 10110|101 10000101 010]11010
n 10010110101101011000010101011010

r 6 21 14
# 100101[10 11000010 1010|1010 110]11010
e 10010110110000101010101011011010
