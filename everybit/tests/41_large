
# Instructions for writing a test:
#
# t: initializes new test
# n: initializes bit array
# r: rotates bit array subset at offset, length by amount
# e: expects raw bit array value

# 0: 30 bit array
t 0

n 100100011000111101010001011101
e 100100011000111101010001011101

r 0 30 90
e 100100011000111101010001011101

r 0 30 -131
r 0 30 11
e 100100011000111101010001011101

r 7 11 2
e 100100001110001111010001011101

r 7 11 -13
e 100100011000111101010001011101

r 6 20 51
r 6 20 -11
e 100100011000111101010001011101

r 0 30 32
e 011001000110001111010100010111

r 0 30 11
e 101000101110110010001100011110
