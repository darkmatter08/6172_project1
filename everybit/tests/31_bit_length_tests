# bit_length = n
t 0

n 000000001
r 0 9 -5
e 000100000

# bit_length = 0
t 1

n 0101010
r 0 3 0
e 0101010

# bit_offset + bit_length = n
t 2

n 000000001
r 4 5 5
e 000000001

# bit_length >> 1
t 3

n 00100100000000000000000000000000000000000000000000
r 2 45 65
e 00000000000000000000001001000000000000000000000000
