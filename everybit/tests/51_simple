#0: bit length = 0
t 0

n 10010110
r 0 0 123
e 10010110

#1: rotate val is a multiple of bit length
t 1

n 1001011011001010111
r 4 7 21
e 1001011011001010111

#2: 7 bit
t 2

n 0111011
r 0 7 12
e 1101101

#3: 9 bit
t 3

n 101010111
r 0 9 2
e 111010101

# 4: all zero
t 4

n 0000000000000000
r 0 15 13
e 0000000000000000

r 1 13 124
e 0000000000000000

#5: all ones
t 5

n 1111111111111111
r 0 14 12
e 1111111111111111

r 1 13 124
e 1111111111111111

#6: rotate value not multiple of length
t 6

n 10111010
r 1 5 2
e 11001110 

n 10101111
r 0 7 3
e 11110101

n 10011001
r 3 4 2
e 10000111

n 10011001
r 4 4 3
e 10010011

