# Testing with variable bit length
t 0
# L = 1

n 0
r 0 0 0 
e 0

r 0 1 0 
e 0

r 0 0 1
e 0

r 0 1 20
e 0

n 1

r 0 1 0
e 1

r 0 1 10
e 1

r 0 0 1
e 1

# L = 2
t 1
n 00
r 0 1 1
e 00

r 0 1 5
e 00

n 10
r 0 1 0
e 10

r 0 1 1
e 10

r 0 2 4
e 10

r 0 2 3
e 01

# L = 4
t 2
n 0000
r 0 4 1
e 0000

r 0 3 2
e 0000

r 0 1 5
e 0000

n 0101
r 0 1 1
e 0101

r 1 3 1
e 0110

# L = 16
t 3
n 0000000000000000

r 0 5 200
e 0000000000000000

n 1111111111111111
r 3 10 300
e 1111111111111111

r 0 16 1
e 1111111111111111

r 4 12 16
e 1111111111111111

r 8 8 5
e 1111111111111111

n 1001000100101010
r 0 8 5
e 1000110000101010

r 0 16 16
e 1000110000101010

r 7 8 4
e 1000110010100010





# odd
t 4
n 000
r 0 2 2
e 000

n 101
r 0 2 1 
e 011

# L = 9
t 5
n 011110001
r 0 5 2
e 110110001

n 000111000
