# Words

# 1: Full word
t 0

n 00101010101111101010011001010011

r 3 0 4
n 00101010101111101010011001010011

r 2 17 9
e 00111110101101010100011001010011

# no rotation; index entire array
r 0 32 32
e 00111110101101010100011001010011

# no rotation; subarray
r 2 17 34
e 00111110101101010100011001010011

# 2: > 1 word, < 2 words
t 1

n 0011111010110101010001100101001011101100101

r 2 24 17
e 0001101010100011001111110101001011101100101

r 2 24 16
e 0010001100111111010110101001001011101100101

