############## 0: bit-length varies
t 0

# bit_length = 0
n 1010011  
r 1 0 4
e 1010011

# bit_length > 0 and even
n 1010011
r 0 4 2
e 1010011

# bit_length > 0 and odd
n 101100101
r 0 5 3
e 110100101

# bit_length > 0 and power of 2
n 101101011101
r 0 8 5
e 101011011101

# bit_length > 64
n 1110010101110010101100101011001010110010101100101011001010110010101
r 0 65 20
e 0101011001010110010111100101011100101011001010110010101100101011001