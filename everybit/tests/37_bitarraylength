############ 0: bitarray length varies
t 0

# bitarray length = 1
n 1
r 0 1 1
e 1

# bitarray length is even
n 0101110110
r 0 10 3
e 1100101110

# bitarray length is odd
n 100100101011010
r 1 14 6
e 101101000100101

# bitarray length is a power of 2
n 01101001
r 0 8 2
e 01011010

# bitarray length > 64
n 010101010101010101010101010101010101010101010101010101010101010110
r 20 40 15
e 010101010101010101011010101010101010101010101010101010101010010110