r 53 18 1
e 10010011100101101011111100001000101011000000011111111100110011011001011110001110110110001010010001110110101011000000101011001100

n 10010011100101101011111100001000101011000000011111111001100110110010111110001110110110001010010001110110101011000000101011001100
r 53 18 5
e 10010011100101101011111100001000101011000000011111111101110011001101100110001110110110001010010001110110101011000000101011001100

n 10010011100101101011111100001000101011000000011111111001100110110010111110001110110110001010010001110110101011000000101011001100
r 53 18 -5
e 10010011100101101011111100001000101011000000011111111011011001011100110110001110110110001010010001110110101011000000101011001100

