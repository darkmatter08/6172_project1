############ 0: bit offset varies
t 0

# bit offset = 0
n 10101011
r 0 8 2
e 11101010

# bit offset = 1
n 1010101010
r 1 6 4
e 1010101010

# bit offset = 2
n 01010101101
r 2 9 3
e 01101010101

# bit offset = near begining
n 10101
r 1 3 1
e 10011

# bit offset = mid
n 1010101010
r 5 5 2
e 1010110010

n 10101110101
r 5 5 3
e 10101010111

# bit offset = near end
n 1101001010101001
r 12 3 2
e 1101001010100011

# bit offset > 0 and even
n 111010111101010110000000001
r 4 6 4
e 111011111001010110000000001

# bit offset > 0 and odd
n 1010110001010110101010
r 13 8 8
e 1010110001010110101010 

# bit offset > 0 and power of 2
n 101000110101101000110101
r 8 5 4
e 101000111011001000110101